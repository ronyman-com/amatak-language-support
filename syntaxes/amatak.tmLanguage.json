{
	"scopeName": "source.amatak",
	"fileTypes": ["amatak"],
	"patterns": [
	  {
		"name": "comment.line.amatak",
		"match": "//.*$"
	  },
	  {
		"name": "comment.block.amatak",
		"begin": "/\\*",
		"end": "\\*/"
	  },
	  {
		"name": "keyword.control.amatak",
		"match": "\\b(let|if|else|while|function|return|print)\\b"
	  },
	  {
		"name": "storage.type.function.amatak",
		"match": "\\bfunction\\b"
	  },
	  {
		"name": "entity.name.function.amatak",
		"match": "\\b[a-zA-Z_][a-zA-Z0-9_]*\\b(?=\\()"
	  },
	  {
		"name": "constant.numeric.amatak",
		"match": "\\b\\d+\\b"
	  },
	  {
		"name": "string.quoted.double.amatak",
		"match": "\"[^\"]*\""
	  },
	  {
		"name": "punctuation.definition.array.amatak",
		"match": "\\[|\\]"
	  },
	  {
		"name": "punctuation.definition.block.amatak",
		"match": "\\{|\\}"
	  }
	]
  }